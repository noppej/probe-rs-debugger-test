{
    "version": "0.2.0",
    "configurations": [
        {
            "preLaunchTask": "nrf52833",
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "probe-rs nrf52833",
            // "server": "127.0.0.1:50001", // For connecting to an existing probe-rs-debugger process
            // TODO: Update with project name
            // TODO: Update target folder
            "cwd": "${workspaceFolder}",
            "programBinary": "${workspaceFolder}/target/thumbv7em-none-eabihf/debug/nrf52833", // TODO: Update with binary name
            "chip": "nRF52833_xxAA", // TODO: Update with chip name
            "coreIndex": 0,
            "runtimeExecutable": "probe-rs-debugger",
            "connectUnderReset": false,
            "flashingEnabled": true,
            "resetAfterFlashing": true,
            "haltAfterReset": false,
            "rttEnabled": true,
            "rttChannelFormats": [
                {
                    "channelNumber": 0,
                    "showTimestamps": true
                }
            ],
            "consoleLogLevel": "Error",
            "speed": 24000 // TODO: Optionally update USB speed
        }
    ]
}